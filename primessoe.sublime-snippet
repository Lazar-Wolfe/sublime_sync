vector<bool> is_prime(sieve+1,true);
is_prime[0]=is_prime[1]=false;
for(int i=2;i*i<=sieve;i++){
	if(is_prime[i]){
		for(int j=i*i;j<=sieve;j+=i){
			is_prime[j]=false;
		}
	}
}
]]></content>
